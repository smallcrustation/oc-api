{
  "name": "oc-api",
  "version": "1.0.0",
  "description": "api for oc-client",
  "main": "server.js",
  "engines": {"node": "10.16.0"},
  "scripts": {
    "start": "node dist/src",
    "start:dev": "nodemon dist/server.js",
    "build:dev": "tsc --watch --preserveWatchOutput",
    "tsc": "tsc",
    "build": "npm run tsc",
    "dev": "npm run build:dev & npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrate": "postgrator --config postgrator-config.ts",
    "migrate:dev": "postgrator --config postgrator-config.ts",
    "migrate:production": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "seed": "psql krill -d oc -f ./seeds/seed.tables.sql"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/helmet": "0.0.45",
    "cloudinary": "^1.21.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "knex": "^0.20.13",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "nodemon": "^2.0.2",
    "postgrator-cli": "^3.3.0"
  }
}
